{
  "public": true,
  "functions": {
    "api/**/*.[jt]s": {
      "runtime": "vercel-deno@3.1.1",
      "maxDuration": 5
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/$1"
    }
  ],
  {
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ]
}
